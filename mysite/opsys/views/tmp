   var username = $.trim($('#nickname1').val());if (username == ''){NewAlert('用户名不能为空！');return false;}var serverid = $.trim($('#serverid_cash').val()); $.ajax({url:'/querytotle/',type:'post',data:{username:username,serverid:serverid, game:'1',area:'6', device:'4'},dataType:'json',success:function(data,status){if (data.ret == 0){NewAlert('处理结果:'+data.msg);}else{NewAlert('失败：'+data.msg);}},error:function(XMLHttpRequest, textStatus, errorThrown){NewAlert('失败：请检查网址是否正确');}});return false;
